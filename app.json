{
  "expo": {
    "name": "Boop the Snoot",
    "description": "Boop the snoot and put a smile on your face. https://boop-snoot.web.app/",
    "slug": "boop-snoot",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.humberto.boopsnoot"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
